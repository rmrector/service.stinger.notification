# Kodi Media Center language file
# Addon Name: Stinger scene notification
# Addon id: service.stinger.notification
msgid ""
msgstr ""

# Notification messages

msgctxt "#32000"
msgid "There is a stinger scene during the credits. Keep watching!"
msgstr "W trakcie napisów końcowych są dodatkowe sceny. Oglądaj dalej."

msgctxt "#32001"
msgid "There is a stinger scene after the credits. Wait for it!"
msgstr "Po napisach końcowych są dodatkowe sceny. Poczekaj na nie."

msgctxt "#32002"
msgid "There are stinger scenes all over the place! Better sit tight."
msgstr "Wszędzie tu występują sceny dodatkowe. Oglądaj uważnie."

msgctxt "#32003"
msgid "During credits"
msgstr "Podczas napisów końcowych"

msgctxt "#32004"
msgid "After credits"
msgstr "Po napisach końcowych"

# Actions

msgctxt "#32400"
msgid "Grab stinger tags from TheMovieDB for all movies"
msgstr "Pobierz tagi dla wszystkich filmów z TheMovieDB"

msgctxt "#32401"
msgid "The tags 'duringcreditsstinger' and 'aftercreditsstinger' have been added to all relevant movies. Thanks, TheMovieDB! Use the Universal Movie Scraper as your movie scraper to tag new movies as they are added."
msgstr "Tagi 'duringcreditsstinger' oraz 'aftercreditsstinger' zostały dodane do odpowiednich filmów dzięki TheMovieDB! Aby automatycznie pobierać tagi dla nowo dodawanych filmów, użyj Universal Movie Scraper jako dostawcy informacji o filmach."

# Settings headings

msgctxt "#32999"
msgid "Consider additional tag for"
msgstr "Użyj dodatkowych tagów dla"
# duringcreditsstinger and aftercreditsstinger are then listed below this heading

msgctxt "#32998"
msgid "Actions"
msgstr "Akcje"

# Settings

msgctxt "#32900"
msgid "Use simple notification message"
msgstr "Użyj prostego okna powiadomienia"

msgctxt "#32901"
msgid "If no chapters available, display notification "
msgstr "Jeśli nie są dostępne informacje o rozdziałach, wyświetl powiadomienie "

msgctxt "#32904"
msgid "Display notification message for "
msgstr "Wyświetl powiadomienie przez "

msgctxt "#32905"
msgid "{0:d} seconds"
msgstr "{0:d} sekund"

msgctxt "#32906"
msgid "{0:d} minutes before end"
msgstr "{0:d} minut przed końcem"
